{
  "name": "rocket-jobs",
  "version": "1.0.0",
  "description": "Example of creating a function that runs as a cron job using the serverless `schedule` event",
  "author": "Dongmin,Yu <ydm2790@gmail.com>",
  "license": "MIT",
  "main": "handler.ts",
  "scripts": {
    "test": "jest",
    "watch": "tsc -w",
    "lint": "npx eslint '*.ts'",
    "start": "ts-node -e \"require('./handler').run()\"",
    "rocket-punch": "ts-node -e \"require('./rocket.main').default()\"",
    "wanted-crawl": "ts-node -e \"require('./wanted.main').default()\"",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "format": "prettier --write *.ts",
    "deploy": "serverless deploy --verbose",
    "info": "serverless info --verbose",
    "invoke": "serverless invoke --function job-collector"
  },
  "keywords": [],
  "dependencies": {
    "@notionhq/client": "^2.2.4",
    "axios": "^1.3.6",
    "cheerio": "1.0.0-rc.12",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@aws-sdk/abort-controller": "^3.310.0",
    "@aws-sdk/client-s3": "^3.312.0",
    "@aws-sdk/lib-storage": "^3.312.0",
    "@types/aws-lambda": "^8.10.114",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.5.0",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "serverless-plugin-typescript": "^2.1.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "aws-sdk": "^2.1359.0",
    "serverless": "^2.32 || 3.2.0"
  }
}
