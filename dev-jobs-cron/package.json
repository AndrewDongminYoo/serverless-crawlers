{
    "name": "rocket-jobs",
    "version": "1.0.0",
    "description": "Example of creating a function that runs as a cron job using the serverless `schedule` event",
    "author": "",
    "license": "MIT",
    "main": "handler.ts",
    "scripts": {
        "test": "jest",
        "watch": "tsc -w",
        "tslint": "tslint '*.ts'",
        "lint": "npx eslint '*.ts'",
        "start": "ts-node -e \"require('./handler').run()\"",
        "rocket-punch": "ts-node -e \"require('./rocket.main').default()\"",
        "wanted-crawl": "ts-node -e \"require('./wanted.main').default()\"",
        "typecheck": "tsc --noEmit",
        "build": "tsc",
        "format": "prettier --write *.ts"
    },
    "keywords": [],
    "dependencies": {
        "@notionhq/client": "^2.2.3",
        "aws-sdk": "^2.1298.0",
        "axios": "^1.2.3",
        "cheerio": "^1.0.0-rc.12",
        "dotenv": "^16.0.3"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.109",
        "@types/dotenv": "^8.2.0",
        "@types/jest": "^29.2.6",
        "@types/mime-types": "^2.1.1",
        "@types/node": "^18.11.18",
        "@typescript-eslint/eslint-plugin": "^5.48.2",
        "jest": "^29.3.1",
        "prettier": "^2.8.3",
        "serverless-plugin-typescript": "^2.1.4",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "tslint": "^6.1.3",
        "typescript": "^4.9.4"
    }
}
